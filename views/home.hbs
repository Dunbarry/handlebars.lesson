<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>{{title}}</title>
  </head>
  <body>
    {{#if cohortName}}
      <h1>{{cohortName}} online.</h1>
    {{else}}
      <h1>Declaring.</h1>
    {{/if}}

    <h1>My favorite foods:</h1>
    {{#each foods}}
    <p>
      {{this}}
    </p>
    {{/each}}
  </body>
</html>
